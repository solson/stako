# Stako is stack-based, inspired by Factor.

twelve: (-- x) 12.

# * is just another word.
sq: (x -- y) dup *.

nl: '\n' putc.

# [] is a quotation.
print: (str --) [putc] each.

println: (str --) print nl.

pprint: (obj --) inspect print.

pprintln: (obj --) pprint nl.

main:
    "Hello, world!" println
    0b1100 sq pprintln.
